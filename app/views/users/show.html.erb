<%= link_to "Logout", logout_path, method: :delete %>

<h1>Welcome, <%= current_user.username %>!</h1>

<h2>Playlist:</h2>
<% @songs.each do |song| %>
  <h3><%= link_to song.title, song_path(song) %></h3>
  <%= link_to "Edit Song", edit_song_path(song) %>
<% end %>

<%= link_to "Create a new song!", new_song_path %>
